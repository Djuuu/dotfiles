{
  "title": "Djuuu custom F13 - F14 - F15",
  "rules": [
    {
      "description": "Print screen selection to file on Control + F13",
      "manipulators": [
        {
          "type": "basic",
          "from": {"key_code": "f13", "modifiers": {"mandatory": ["control"]}},
          "to": [{"key_code": "4", "modifiers": ["left_shift", "left_command"]}]
        }
      ]
    },
    {
      "description": "Print screen selection to clipboard on F13",
      "manipulators": [
        {
          "type": "basic",
          "from": {"key_code": "f13"},
          "to": [{"key_code": "4", "modifiers": ["left_control", "left_shift", "left_command"]}]
        }
      ]
    },
    {
      "description": "Lock screen on F14",
      "manipulators": [
        {
          "type": "basic",
          "from": {"key_code": "f14", "modifiers": {"optional": ["any"]}},
          "to": [{"key_code": "a", "modifiers": ["left_control", "left_command"]}]
        }
      ]
    }
  ]
}
