{
  "title": "Djuuu custom program shortcuts",
  "rules": [
    {
      "description": "[Dju Pgm] Open Activity Monitor on Shift + Control + Esc",
      "manipulators": [{
        "type": "basic",
        "from": {"key_code": "escape", "modifiers": {"mandatory": ["control", "shift"]}},
        "to": [{"shell_command": "open -a 'Activity Monitor.app'"}]
      }]
    }
  ]
}
